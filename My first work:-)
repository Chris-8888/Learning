for(int i = 5; i > 0; i--) {
    System.out.print("@");
    for (int j = 1; j < i; j++) {
        System.out.print(" ");
    }
    System.out.println("@");
}
System.out.println("@");

for(int i = 5; i > 0; i--) {
    System.out.print("@");
    for (int j = 5; j > i; j--) {
        System.out.print(" ");
    }
    System.out.println("@");
}
